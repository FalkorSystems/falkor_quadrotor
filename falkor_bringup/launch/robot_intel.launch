<?xml version="1.0" ?>
<launch>
  <include file="$(find falkor_quadrotor_sensors)/launch/imu_gps_tools.launch" />
  <include file="$(find falkor_quadrotor_control)/launch/robot.launch" />
  <include file="$(find falkor_quadrotor_urdf)/launch/for_tf.launch" >
     <arg name="model" value="$(find falkor_quadrotor_urdf)/urdf/quadrotor_with_cam.urdf.xacro"/> 
     <arg name="model_name" value="robot" />
  </include>

  <node pkg="falkor_quadrotor_sensors" name="mag_to_tf" type="mag_to_tf.py">
    <param name="declination" value="-13.083" />
    <param name="world_frame" value="/map" />
  </node>
</launch>
